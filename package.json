        "lodash": "^4.17.21",
        "postcss": "^7",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-scripts": "4.0.3",
        "tailwindcss": "npm:@tailwindcss/postcss7-compat",
        "web-vitals": "^1.0.1"
    },
    "scripts": {
        "lint": "eslint \"**/*.{ts,tsx}\"",
        "deploy": "craco build & gh-pages -d build",
        "generate": "graphql-codegen --config codegen.js",
        "storybook": "start-storybook -p 6006 -s public",
        "build-storybook": "build-storybook -s public",
        "start": "craco start",
        "build": "craco build",
        "test": "craco test"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ],
        "overrides": [
            {
                "files": [
                    "**/*.stories.*"
                ],
                "rules": {
                    "import/no-anonymous-default-export": "off"
                }
            }
        ]
    },
    "resolutions": {
        "babel-loader": "8.1.0"
    },
    "devDependencies": {
        "@storybook/addon-actions": "^6.4.7",
        "@storybook/addon-essentials": "^6.4.7",
        "@storybook/addon-links": "^6.4.7",
        "@storybook/node-logger": "^6.4.7",
        "@storybook/preset-create-react-app": "^3.2.0",
        "@storybook/react": "^6.4.7",
        "@types/jest": "^26.0.15",
        "@types/lodash": "^4.14.177",
        "@types/node": "^12.0.0",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "@typescript-eslint/eslint-plugin": "^5.5.0",
        "@typescript-eslint/parser": "^5.5.0",
        "eslint-config-airbnb": "^19.0.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.27.1",
        "eslint-plugin-react-hooks": "^4.3.0",
        "prettier": "^2.5.0",
        "react-test-renderer": "^17.0.2",
        "typescript": "^4.2.0"
    }
